
public class Homework1 {
	
	public static void main(String[] args) {
		
		int f = 90;
		float TempCalc = (f - 32) * 5/9; 
		
		if (TempCalc > 0) {
			System.out.println("the temp is " +f+ " Farenheit, or " + TempCalc + " Degrees");
		}
		else {
			System.out.println("the temp is " +f+ " Farenheit, or " + TempCalc + " Degrees");
		}
	}

}
